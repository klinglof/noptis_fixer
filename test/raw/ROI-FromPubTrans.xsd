	<!--======================================================================================== -->
	<xs:include schemaLocation="ROI-SharedTypes.xsd"/>
	<xs:element name="FromPubTransMessages" type="ROI:FromPubTransMessages"/>
	<!--======================================================================================== -->
	<xs:complexType name="FromPubTransMessages">
		<xs:complexContent>
			<xs:extension base="PT:Messages">
				<xs:choice maxOccurs="unbounded">
					<xs:group ref="PT:SystemMessages"/>
					<xs:group ref="ROI:VehicleJourneyCreateEvents"/>
					<xs:group ref="ROI:VehicleJourneyDeleteEvents"/>
					<xs:group ref="ROI:VehicleJourneyUpdateEvents"/>
					<xs:group ref="ROI:DeviationCaseCreateEvents"/>
					<xs:group ref="ROI:DeviationCaseUpdateEvents"/>
					<xs:group ref="ROI:OtherEvents"/>
					<xs:group ref="ROI:SubscriptionMessages"/>
				</xs:choice>
				<xs:attribute name="DocumentLayoutVersion" type="ROI:FromDocumentLayoutVersion" use="required"/>
				<xs:attribute name="LocalTimeZoneOffsetTime" type="xs:duration" use="required">
					<xs:annotation>
						<xs:documentation>Describes the local time zone offset without compensation for daylight saving time ( i.e. always +1 hour for Sweden)</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="LocalLanguage" type="xs:language" use="required">
					<xs:annotation>
						<xs:documentation>Describes the default language for texts</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!-- -->
	<xs:group name="SubscriptionMessages">
		<xs:choice>
			<xs:element name="SynchronisationReport" type="ROI:SynchronisationReport"/>
