			<xs:element name="DepartureUpdateEvent" type="ROI:DepartureUpdateEvent"/>
			<xs:element name="VehicleJourneyDeviationEvent" type="ROI:VehicleJourneyDeviationEvent"/>
			<xs:element name="ArrivalDeviationEvent" type="ROI:ArrivalDeviationEvent"/>
			<xs:element name="DepartureDeviationEvent" type="ROI:DepartureDeviationEvent"/>
